doctype html
html(lang="es")
    block config
        include ../../config/var/var.pug
    head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible", content="IE=edge")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        title #{page[0]}
        script(src='menu.js')
        each link in css
            link(rel="stylesheet" href='../../css/' + link)
        link(rel="preconnect", href="https://fonts.googleapis.com")
        link(rel="preconnect", href="https://fonts.gstatic.com" crossorigin)
        link(href="https://fonts.googleapis.com/css2?family=EB+Garamond:wght@400;700;800&family=League+Spartan:wght@400;800&display=swap" rel="stylesheet")

    body 
        block content



